    ],
    "ncpdp": {
      "general": "inputs/ncpdp_general_standards.json",
      "script": "inputs/ncpdp_script_standards.json"
    },
    "ncpdp_filter": {
      "standards": ["T", "F", "H", "A"]
    },
    "other": []
  },
  "output": {
    "directory": "output/plan_and_benefit",
    "filename": "Plan_and_Benefit_CDM.xlsx"
  }
}